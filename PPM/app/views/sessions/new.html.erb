<div class="container">
  <div class="span5 offset1">
  <div class="frosty">

  <h2 class="custom-h2">Log In</h2>
  </br>

  <%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email, "Email", :class => "custom-h3" %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password, "Password", :class => 'custom-h3' %><br />
    <%= password_field_tag :password %>
  </p>
      <%= submit_tag("Sign Up", :class => 'btn btn-large btn-success') %>
<% end %>

  </div>
  </div>
</div>